<link rel="stylesheet" href="./style/main.css">
<iframe src="./slideshow.html" class="slideshow"></iframe>
<div class="bestsellers-label">BESTSELLERS</div>
<div class="bestsellers">
    <div class="bestsellers-left-space"></div>
    <div class="bestsellers-right-space"></div>
    <div class="bestseller-first-line"></div>
    <% for(var ii = 0; ii < bestsellers.length/8; ++ii ){ %>
        <div class="bestsellers-line" data-bestseller-number="<%= ii+1 %>">
            <% for(var cc = 0; cc < 8; ++cc){%>
                <a href="<%= '/product/'+bestsellers[ii*8+cc].productCode %>">
                    <div class="bestsellers-item" data-code="<%= bestsellers[ii*8+cc].productCode %>">
                        <div class="bestseller-title"><%= bestsellers[ii*8+cc].title %></div>
                        <div class="bestseller-subtitle"><a href="<%= '/search/'+ bestsellers[ii*8+cc].category.toLowerCase() %>"><%= bestsellers[ii*8+cc].category %></a></div>
                        <img class="bestseller-image" src="/images/products/<%= bestsellers[ii*8+cc].productCode+"/"+bestsellers[ii*8+cc].image[0] %>">
                        <div class="bestseller-price"><%= bestsellers[ii*8+cc].Price %></div>
                        <div class="bestseller-overflow">
                            <div class="bestseller-addtobag">
                                Add to Bag >
                            </div>
                            <div class="bestseller-wishlist">‚ù§ Add to wishlist</div>
                        </div>
                    </div>
                </a>
            <% }; %>
        </div>
    <% }; %>
</div>
<div class="bestsellers-list-select" data-bestsellers-count="<%= bestsellers.length/8 %>">
    <img width="13px" height="22px" src="./img/_left.png" id="bestsellers_prew_page">
    <input type="radio" id="rad1" name="rads" data-radio-bestseller-number="1" checked="true">
    <label for="rad1"></label>
    <% for(var ii = 2; ii <= bestsellers.length/8; ++ii ){ %>
        <input type="radio" id="rad<%= ii %>" name="rads" data-radio-bestseller-number="<%= ii %>">
        <label for="rad<%= ii %>"></label>
    <% }; %>

        <!--
        <input type="radio" id="rad3" name="rads" data-radio-bestseller-number="3">
        <label for="rad3"></label> -->
    <img width="13px" height="22px" src="./img/_right.png" id="bestsellers_next_page">
</div>

